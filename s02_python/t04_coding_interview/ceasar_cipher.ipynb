    "import string\n",
    "\n",
    "def ceasar_cipher(s, n):\n",
    "    letters = string.ascii_lowercase\n",
    "    output = ''\n",
    "    for c in s:\n",
    "        if c in letters:\n",
    "            output += letters[(letters.find(c) + n) % 26]\n",
    "        else:\n",
    "            output += c\n",
    "    return output\n",
    "\n",
    "\n",
    "ceasar_cipher('abcd xyz', 4)"
   ]
